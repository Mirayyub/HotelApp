@model Category
@{
    ViewBag.Title = "Create";
}
<div class="page">
    <div id="category">

        <div class="header">

            <div class="col-md-4">
                <h1>Kategoriya</h1>
            </div>



        </div>



        <div class="create">
            <form method="post">

                <div class="form-group col-md-4">
                    @Html.LabelFor(m => m.CategoryName)
                    @Html.TextBoxFor(m => m.CategoryName, null, new { @class = "form-control" })
                    @Html.ValidationMessageFor(m => m.CategoryName, "", new { @class = "text-danger" })
                </div>
                <div class="form-group">
                    <div class="col-md-12">
                        <button type="submit" class="btn btn-success">Əlavə Et</button>

                    </div>
                </div>
            </form>
        </div>
        @Html.ActionLink("Geriyə qayıt", "index")
    </div>
</div>