@model HotelsA.Models.BedType
@{
    ViewBag.Title = "Create";
}

<div class="page">
    <div id="bedtype">

        <div class="header">
            <div class="row">
                <div class="col-md-3">
                    <h1>Yataq Növü</h1>
                </div>

            </div>
        </div>


        <div class="create">
            <form method="post">
                <div class="form-group col-md-3">
                    @Html.LabelFor(m => m.TypeName)
                    @Html.TextBoxFor(m => m.TypeName, null, new { @class = "form-control" })
                    @Html.ValidationMessageFor(m => m.TypeName, "", new { @class = "text-danger" })
                </div>
                <div class="form-group">
                    <div class="col-md-3">
                        <button type="submit" class="btn btn-success">Əlavə et</button>

                    </div>
                </div>
            </form>
        </div>
        @Html.ActionLink("Geriyə qayıt", "index")
    </div>
</div>
