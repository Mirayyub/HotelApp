@model UserRol
@{
    ViewBag.Title = "Create";
}

<div class="page">
    <div id="userrol">

        <div class="header">

            <div class="col-md-4">
                <h1>Vəzifə Əlavə et</h1>
            </div>


        </div>


        <div class="create">
            <form method="post">

                <div class="form-group col-md-3">
                    @Html.LabelFor(m => m.UserType)
                    @Html.TextBoxFor(m => m.UserType, null, new { @class = "form-control" })
                    @Html.ValidationMessageFor(m => m.UserType, "", new { @class = "text-danger" })
                </div>
                <div class="form-row">
                    <div class="col-md-4">
                        <button type="submit" class="btn btn-success">Əlavə et</button>

                    </div>
                </div>
            </form>
        </div>
        @Html.ActionLink("Geriyə qayıt", "index")
    </div>
</div>